<?xml version="1.0" encoding="UTF-8"?>
<odoo>

  <record model="ir.ui.view" id="data_customerpersonal_tree">
    <field name="name">data.customerpersonal.tree</field>
    <field name="model">data.customerpersonal</field>
    <field name="priority" eval="1"/>
    <field name="arch" type="xml">
      <tree>
        <field name="name"/>
        <field name="nik"/>
        <field name="phone_number"/>
      </tree>
    </field>
  </record>

  <record model="ir.ui.view" id="data_branchoffice_tree">
    <field name="name">data.branchoffice.tree</field>
    <field name="model">data.branchoffice</field>
    <field name="priority" eval="1"/>
    <field name="arch" type="xml">
      <tree>
        <field name="code"/>
        <field name="name"/>
      </tree>
    </field>
  </record>

  <record model="ir.ui.view" id="data_account_tree">
    <field name="name">data.account.tree</field>
    <field name="model">data.account</field>
    <field name="priority" eval="1"/>
    <field name="arch" type="xml">
      <tree>
        <field name="account_number"/>
        <field name="type"/>
      </tree>
    </field>
  </record>

  <record model="ir.ui.view" id="data_correspondence_tree">
    <field name="name">data.correspondence.tree</field>
    <field name="model">data.correspondence</field>
    <field name="priority" eval="1"/>
    <field name="arch" type="xml">
      <tree>
        <field name="title"/>
        <field name="submitted_by"/>
      </tree>
    </field>
  </record>

  <record model="ir.ui.view" id="data_loan_tree">
    <field name="name">data.loan.tree</field>
    <field name="model">data.loan</field>
    <field name="priority" eval="1"/>
    <field name="arch" type="xml">
      <tree>
        <field name="account_number"/>
        <field name="type"/>
      </tree>
    </field>
  </record>

  <record model="ir.ui.view" id="data_loan_tree">
    <field name="name">data.asset.tree</field>
    <field name="model">data.asset</field>
    <field name="priority" eval="1"/>
    <field name="arch" type="xml">
      <tree>
        <field name="asset_name"/>
        <field name="type"/>
        <field name="asset_value"/>
        <field name="date_acquired"/>
      </tree>
    </field>
  </record>

</odoo>